package philips.healthcare.radar.rules.mrpostprocessrules;

import philips.healthcare.radar.rules.mrpostprocessrules.MRMeasurementDataModel

rule "HeliumOOS"
	when
		m :  MRMeasurementDataModel( mHeliumValue > MRMeasurementDataModel.HELIUMOOSLIMIT)
	then
		System.out.println("Helium out of spec");
		m.SetIsHeliumOOS(1);
		update(m);	
end